{
  "name": "explorer-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0",
    "build:lib": "ng build ngx-explorer && npm run copy",
    "build:watch": "ng build ngx-explorer --watch",
    "build": "ng build",
    "build:gh": "ng build --base-href=/ngx-explorer/",
    "pack": "rm -rf dist && ng build ngx-explorer && npm run copy && cd dist/ngx-explorer && npm pack",
    "copy": "cp README.md dist/ngx-explorer/",
    "test": "ng test",
    "prettier": "prettier --write .",
    "lint": "eslint . --ext .ts --fix",
    "semantic-release": "semantic-release"
  },
  "private": false,
  "license": "Apache-2.0",
  "dependencies": {
    "@angular/animations": "17.2.0",
    "@angular/cdk": "17.2.0",
    "@angular/common": "17.2.0",
    "@angular/compiler": "17.2.0",
    "@angular/core": "17.2.0",
    "@angular/forms": "17.2.0",
    "@angular/material": "17.2.0",
    "@angular/platform-browser": "17.2.0",
    "@angular/platform-browser-dynamic": "17.2.0",
    "@angular/router": "17.2.0",
    "@material/dialog": "14.0.0",
    "chart.js": "^4.3.0",
    "chartjs-plugin-annotation": "^3.0.1",
    "ml-regression-simple-linear": "^3.0.1",
    "ng2-charts": "^6.0.1",
    "ngx-explorer": "5.0.1",
    "regression": "^2.0.1",
    "rxjs": "~7.8.1",
    "ts-md5": "^1.3.1",
    "tslib": "^2.6.3",
    "zone.js": "~0.14.7"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.8",
    "@angular/cli": "17.2.0",
    "@angular/compiler-cli": "17.2.0",
    "@types/jasmine": "~5.1.4",
    "@types/regression": "^2.0.6",
    "jasmine-core": "~5.1.2",
    "karma": "^6.4.2",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^17.0.0",
    "prettier": "^3.3.2",
    "typescript": "~5.2"
  }
}
